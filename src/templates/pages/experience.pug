                                        -const expLicense2 = {img: '392', title: 'General Securities Qualification Certificate (CME-1)', place: '• Capital Market Authority'}
                                        -const expLicense2_1 = {img: '392', title: 'Wealth Management', place: '• Capital Market Authority'}
                                        -const expLicense2_2 = {img: '392', title: 'Corporate Finance', place: '• Capital Market Authority'}
                                        -const expLicense3 = {img: '393', title: 'Entrepreneurship Adventures', place: ' KAUST (King Abdullah University of Science and Technology'}
                                        -const expLicense4 = {img: '394', title: 'Certified Wealth Management Diploma', place: 'Swiss Association for Quality (SAQ)'}
                                        -const expLicense5 = {img: '395', title: 'Accredited SME Consultant™', place: 'Association of Accredited Small Business Consultants ®'}
                                        -const expLicense6 = {img: '396', title: 'Project Develop Cycle (Preparation & Appraisal of Investment Project)', place: 'UNIDO'}
                                        -const expLicense7 = {img: '397', title: 'Job Family Modeling & Job Analysis Certificate', place: 'Hay Group'}
                                        -const expLicense8 = {img: '398', title: 'Remuneration Committee Workshop and Update', place: 'Non-Executive Directors\'​ Association'}
                                        -const expLicense9 = {img: '399', title: 'Blockchain Technologies: Business Innovation and Application', place: 'MIT Sloan School of Management'}
                                        -const expLicense10 = {img: '400', title: 'Private Equity Specialist Certificate', place: 'International Academy of Financial Management'}
                                        -const expLicense11 = {img: '401', title: 'Project Quality & Logical Framework, EDI', place: 'The World Bank'}
                                        -const expLicense12 = {img: '402', title: 'HRM Policies & Procedures Program Certificate', place: 'Cranfield School of Management'}
                                        -const expLicense13 = {img: '403', title: 'Management Development Program Certificate', place: 'London Business School'}

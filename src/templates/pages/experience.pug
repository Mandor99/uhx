                                        -const expLicense19_1 = {img: '409', title: 'Oxford Entrepreneurship: Venture Finance Programme Certificate', place: 'Saïd Business School, University of Oxford'}
                                        -const expLicense20 = {img: '410', title: 'Exercising Leadership: Foundational Principles Certificate', place: ' Harvard University'}

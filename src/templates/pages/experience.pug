                                        -const expLicense22 = {img: '412', title: 'Chartered Director', place: 'Institute of Directors (IoD)'}
                                        -const expLicense22_1 = {img: '412', title: 'IOD Diploma in Company Direction', place: 'Institute of Directors (IoD)'}
                                        -const expLicense22_2 = {img: '412', title: 'IOD Certificate in Company Direction', place: 'Institute of Directors (IoD)'}
                                        -const expLicense23 = {img: '413', title: 'Open Banking, PSD2, and GDPR Certificate', place: 'Udemy'}
                                        -const expLicense23_1 = {img: '413', title: 'Digital Banking - Masterclass', place: 'Udemy'}
                                        -const expLicense24 = {img: '414', title: 'Digital Transformation', place: ' University of Virginia Darden School of Business'}
                                        -const expLicense25 = {img: '415', title: 'Remuneration Committee Workshop and Update', place: 'Mercer'}
                                        -const expLicense26 = {img: '416', title: 'Property Development and Investment', place: ' University of Cape Town'}
                                        -const expLicense27 = {img: '417', title: 'Remuneration Committees Training Course', place: ' 21 st Century'}
                                        .carousel-item.active
                                            .row.justify-content-center.pb-5
                                                -const allExpLicenses = [expLicense1, expLicense2, expLicense2_1, expLicense2_2, expLicense3, expLicense4, expLicense5, expLicense6, expLicense7, expLicense8, expLicense9, expLicense10]
                                                each expLicense in allExpLicenses
                                                    .col-xs-12.col-md-6.col-lg-4.mb-3
                                                        .card.align-items-center
                                                            .row.card-row.flex-row-reverse
                                                                img.col-md-4.img-fluid.exp-img1(src=`../imgs/licenses/Asset ${expLicense.img}.png`, alt="exp")
                                                                .body.col-md-8.text-left.d-flex.flex-column.justify-content-center.text-start
                                                                    h6.card-title #{expLicense.title}
                                                                    p.card-text: small.text-secondary #{expLicense.place}

                                        .carousel-item
                                            .row.justify-content-center.pb-5
                                                -const allExpLicenses2 = [expLicense11, expLicense12, expLicense13, expLicense14, expLicense15, expLicense16, expLicense17, expLicense18, expLicense18_1, expLicense19, expLicense19_1, expLicense20]
                                                each expLicense in allExpLicenses2
                                                    .col-xs-12.col-md-6.col-lg-4.mb-3
                                                        .card.align-items-center
                                                            .row.card-row.flex-row-reverse
                                                                img.col-md-4.img-fluid.exp-img1(src=`../imgs/licenses/Asset ${expLicense.img}.png`, alt="exp")
                                                                .body.col-md-8.text-left.d-flex.flex-column.justify-content-center.text-start
                                                                    h6.card-title #{expLicense.title}
                                                                    p.card-text: small.text-secondary #{expLicense.place}


                                        .carousel-item
                                            .row.justify-content-center.pb-5
                                                -const allExpLicenses3 = [expLicense21, expLicense22, expLicense22_1, expLicense22_2, expLicense23, expLicense23_1, expLicense24, expLicense25, expLicense26, expLicense27]
                                                each expLicense in allExpLicenses3
                                                    .col-xs-12.col-md-6.col-lg-4.mb-3
                                                        .card.align-items-center
                                                            .row.card-row.flex-row-reverse
                                                                img.col-md-4.img-fluid.exp-img1(src=`../imgs/licenses/Asset ${expLicense.img}.png`, alt="exp")
                                                                .body.col-md-8.text-left.d-flex.flex-column.justify-content-center.text-start
                                                                    h6.card-title #{expLicense.title}
                                                                    p.card-text: small.text-secondary #{expLicense.place}



    include ../components/footer